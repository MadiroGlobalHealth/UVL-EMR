        "id": "demographics",
        "fields": ["name", "gender", "dob", "id"]
      },
      {
        "id": "contact",
        "name": "Contact Details",
        "fields": ["address", "telephoneNumber"]
      },
      {
        "id": "additionalInformation",
        "name": "Additional Information",
        "fields": ["maritalStatus", "occupation"]
      }
