        "uuid": "14d4f066-15f5-102d-96e4-000c29c2a5d7",
        "label": "Telephone Number",
        "validation": {
          "required": false,
          "matches": "^\\+?(\\d{1,3})?[-.\\s]?(\\(?\\d{1,4}?\\)?)?[-.\\s]?\\d{1,4}[-.\\s]?\\d{1,4}[-.\\s]?\\d{1,9}$"
